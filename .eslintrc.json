{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "indent": ["error", 2],
        "quotes": ["error", "single"],
        "linebreak-style": ["error", "unix"]
    }
  }